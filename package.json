{
  "name": "cog-salesforce",
  "version": "0.7.0",
  "description": "Salesforce Cog for use with Crank",
  "LICENSE": "MIT",
  "cog": {
    "name": "automatoninc/salesforce",
    "label": "Salesforce",
    "homepage": "https://github.com/run-crank/cog-salesforce",
    "authHelpUrl": "https://help.salesforce.com/articleView?id=connected_app_create.htm&type=5#oauth"
  },
  "scripts": {
    "build-docker": "docker build -t automatoninc/salesforce:$npm_package_version -t automatoninc/salesforce:latest .",
    "build-proto": "scripts/build-proto.sh",
    "build-ts": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "check-engine package.json && ts-node src/core/grpc-server.ts",
    "test": "nyc mocha -r ts-node/register test/*.ts test/**/*.ts test/**/**/*.ts",
    "version": "crank cog:readme automatoninc/salesforce && git add README.md"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "**/*.js",
      "src/core/grpc-server.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.1",
    "@types/google-protobuf": "^3.2.7",
    "@types/jsforce": "^1.9.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "check-engine": "^1.10.1",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^2.5.1",
    "mocha": "^7.2.0",
    "nyc": "^14.1.1",
    "sinon-chai": "^3.3.0",
    "ts-sinon": "^1.0.17",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@run-crank/utilities": "^0.5.2",
    "ajv": "^6.12.6",
    "async": "^2.6.4",
    "decode-uri-component": "^0.2.2",
    "flat": "^5.0.2",
    "glob-parent": "^5.1.2",
    "google-protobuf": "^3.8.0",
    "grpc": "^1.24.11",
    "handlebars": "^4.7.7",
    "highlight.js": "^9.18.5",
    "hosted-git-info": "^2.8.9",
    "ini": "^1.3.8",
    "jsforce": "^1.9.3",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "minimatch": "^3.1.2",
    "minimist": "^1.2.7",
    "moment": "^2.29.4",
    "path-parse": "^1.0.7",
    "pathval": "^1.1.1",
    "qs": "^6.5.3",
    "redis": "^3.1.2",
    "set-getter": "^0.1.1",
    "striptags": "^3.2.0",
    "title-case": "^3.0.2",
    "ts-node": "^8.3.0",
    "y18n": "^3.2.2"
  }
}
